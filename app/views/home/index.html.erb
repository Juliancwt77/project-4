    <section id ="organisations" class="section-padding">
      <div class="container">
        <div class="header-section text-center">
          <h2>Pricing</h2>
        </div>
        <div class="row">
          <div class="col-md-6">
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
              <div class="orga-stru">
                <h3>Free <br> &nbsp; <br> &nbsp; </h3>
                <p>Limited Access</p>
                <i class="fa fa-male"></i>
              </div>
            </div>
            <div class="col-lg-4 col-md-4 col-sm-4 col-xs-4">
              <div class="orga-stru">
                <h3>$10 <br> per <br> mth</h3>
