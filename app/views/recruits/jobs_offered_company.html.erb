<div class="container">
  <div class="row">

    <div class="heading heading-contact">
      <div class="col-lg-12">
        <div class="section-heading">
          <h2>Jobs Offered</h2>
        </div>
      </div>
    </div>

    <div class="heading col-lg-12">





    </div>

    <div class="col-md-12">
      <table class="table table-list-search">
        <thead>
          <tr>
            <th>Date Sent</th>
            <th>Username</th>
            <th>Title</th>
            <th>Skills</th>
            <th>Rate</th>
            <th>Start Date</th>
            <th>End Date</th>
            <th>Scope</th>
            <th>Message</th>
            <th>Status/Action</th>
          </tr>
        </thead>

        <tbody>
          <% @offers_sent.reverse_order.each do |offer_sent| %>
              <tr>
                  <td> <%= offer_sent.updated_at %> </td>
                  <td> <%= offer_sent.posting.candidate.individual.username %> </td>
                  <td> <%= offer_sent.posting.candidate.title %> </td>
                  <td> <%= offer_sent.posting.candidate.language %> </td>
                  <td> <%= offer_sent.posting.candidate.rate %> </td>
                  <td> <%= offer_sent.start_date %> </td>
                  <td> <%= offer_sent.end_date %> </td>
                  <td> <%= offer_sent.scope %> </td>
                  <td> Message Here!!! </td>
                  <td> Action Here!!! </td>
              <tr>
          <% end %>
        <tbody>

      </table>
    </div>
  </div>
</div>
